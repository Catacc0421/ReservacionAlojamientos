<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="387.0" prefWidth="620.0" style="-fx-background-color: #9ef5aa; -fx-spacing: 30; -fx-padding: 30;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.reservacionAlojamientos.controladores.RegistroControlador">
   <children>
      <Label text="Panel de registro del cliente">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-spacing: 20;">
         <children>
            <GridPane prefHeight="203.0" prefWidth="380.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="144.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="270.0" minWidth="10.0" prefWidth="270.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="44.4" minHeight="10.0" prefHeight="33.4" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="36.39999999999999" minHeight="10.0" prefHeight="35.39999999999999" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="44.4" minHeight="10.0" prefHeight="33.4" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Nombre completo:">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Cédula:" GridPane.rowIndex="1">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Teléfono:" GridPane.rowIndex="2">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Email:" GridPane.rowIndex="3">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Contraseña:" GridPane.rowIndex="4">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtNombre" GridPane.columnIndex="1" />
                  <TextField fx:id="txtCedula" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="txtTelefono" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="txtEmail" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <PasswordField fx:id="txtContrasena" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <ImageView fitHeight="171.0" fitWidth="151.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/Book.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <Label text="Si ya estás registrado vuelve al inicio para iniciar sesión">
         <font>
            <Font name="System Bold" size="10.0" />
         </font>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-padding: 20; -fx-spacing: 60;">
         <children>
            <Region prefHeight="37.0" prefWidth="34.0" />
            <Button fx:id="buttonVolver" mnemonicParsing="false" onAction="#volver" prefHeight="46.0" prefWidth="125.0" style="-fx-background-color: #25d03d;" text="Volver al inicio">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="buttonRegistrarme" mnemonicParsing="false" onAction="#realizarRegistro" prefHeight="45.0" prefWidth="142.0" style="-fx-background-color: #25d03d;" text="Registrarme">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
